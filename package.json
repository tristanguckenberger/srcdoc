{
	"name": "srcdoc",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"launch": "npm run dev -- --open",
		"build": "vite build",
		"build:production": "npm install dotenv && vite -r dotenv/config build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/enhanced-img": "^0.1.8",
		"@sveltejs/kit": "^2.5.11",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/svelte": "^5.1.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"jest-dom": "^4.0.0",
		"jsdom": "^24.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^5.0.0-next.243",
		"svelte-check": "^3.4.3",
		"typescript": "^5.0.0",
		"vite": "^5.3.0",
		"vitest": "^0.32.2",
		"workbox-precaching": "^7.0.0"
	},
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@vercel/analytics": "^1.1.2",
		"@vercel/blob": "^0.23.4",
		"dotenv": "^16.3.1",
		"embla-carousel-svelte": "^8.0.0-rc17",
		"embla-carousel-wheel-gestures": "^8.0.0-rc05",
		"html-to-image": "^1.11.11",
		"html2canvas": "^1.4.1",
		"lodash-es": "^4.17.21",
		"monaco-editor": "^0.51.0",
		"split.js": "^1.6.5",
		"svelte-gestures": "^1.5.2",
		"svelte-json-tree": "^1.0.0",
		"svelte-kit-cookie-session": "^4.0.0",
		"svelte-preprocess": "^5.0.4"
	},
	"type": "module"
}
