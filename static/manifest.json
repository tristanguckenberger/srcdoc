{
	"name": "Play Engine",
	"start_url": "/",
	"display": "fullscreen",
	"theme_color": {
		"default": "#ffffff",
		"dark": "#000000",
		"light": "#ffffff"
	},
	"background_color": "#ffffff",
	"icons": [
		{
			"sizes": "192x192",
			"src": "icon192.png",
			"type": "image/png"
		},
		{
			"sizes": "512x512",
			"src": "icon512.png",
			"type": "image/png"
		}
	],
	"share_target": {
		"action": "/share-target",
		"method": "POST",
		"enctype": "multipart/form-data",
		"params": {
			"title": "name",
			"text": "description",
			"url": "link"
		}
	}
}
